{
    "version": 2,
    "builds": [
      {
        "src": "wsgi.py",
        "use": "@vercel/python",
        "config": {
        "excludeFiles": "/{.next,*.cache,node_modules,public,app}/**"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "wsgi.py"
      }
    ]
  }
